{
  "description": "Pipeline for parsing TiDB slowlog logs",
  "processors": [{
    "set": {
      "field": "event.ingested",
      "value": "{{_ingest.timestamp}}"
    }
  }],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
